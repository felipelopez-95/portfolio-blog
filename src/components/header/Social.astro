---
import githubIcon from '../../assets/social/github.svg'
import linkedinIcon from '../../assets/social/linkedin.svg'
import instagramIcon from '../../assets/social/instagram.svg'
---

<div>
    <a href="#">
        <span>
            <img src={githubIcon.src} alt="GitHub" width="24" height="24">
        </span>
    </a>
    <a href="#">
        <span>
            <img src={linkedinIcon.src} alt="GitHub" width="24" height="24">
        </span>
    </a>
    <a href="#">
        <span>
            <img src={instagramIcon.src} alt="GitHub" width="24" height="24">
        </span>
    </a>
</div>